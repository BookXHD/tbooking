<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tzg.xhd.tbooking.mapper.provinceMapper">
    <resultMap id="ProResultMap" type="com.tzg.xhd.tbooking.entity.province">
        <result property="provinceId" jdbcType="INTEGER" column="province_id"/>
        <result property="provinceName" jdbcType="VARCHAR" column="province_name" />
        <result property="provinceDirec" jdbcType="VARCHAR" column="province_direc" />
        <result property="provinceArea" jdbcType="FLOAT" column="province_area" />
    </resultMap>

    <select id="getAllProvince" resultMap="ProResultMap" >
        SELECT * FROM province
    </select>
</mapper>